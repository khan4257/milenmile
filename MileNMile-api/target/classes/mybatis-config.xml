<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
		<!--<setting name="lazyLoadingEnabled" value="true" />-->
		<setting name="aggressiveLazyLoading" value="true" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<!-- <setting name="autoMappingBehavior" value="false" /> -->
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="defaultStatementTimeout" value="5000" />
	</settings>
	<typeAliases>
		<typeAlias type="com.appdisco.mnm.model.User" alias="User"/>
		<typeAlias type="com.appdisco.mnm.model.Role" alias="Role"/>
		<typeAlias type="com.appdisco.mnm.model.EndUser" alias="EndUser"/>
		<typeAlias type="com.appdisco.mnm.model.EndUserProfile" alias="EndUserProfile"/>
		<typeAlias type="com.appdisco.mnm.model.EndUserAccess" alias="EndUserAccess"/>
		<typeAlias type="com.appdisco.mnm.model.EndUserSession" alias="EndUserSession"/>
		<typeAlias type="com.appdisco.mnm.model.vo.EndUserVO" alias="EndUserVO"/>
		<typeAlias type="com.appdisco.mnm.common.dto.SearchOps" alias="SearchOps"/>
	</typeAliases>
</configuration>
